{
  "name": "reify",
  "version": "1.0.2",
  "description": "Crank out the basic scaffolding necessary for common React / Redux components",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "start": "babel-node src/index.js",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm run remove-dist && mkdirp ./dist",
    "build": "babel src --presets babel-preset-latest --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint --ext=.js src test tests",
    "watch:lint": "esw -w --ext=.js src test",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "reify": "./dist/index.js"
  },
  "keywords": [
    "React",
    "Redux",
    "CLI"
  ],
  "author": "Joe Costanzo, RentPath, et al",
  "repository": {
    "type": "git",
    "url": "https://github.com/rentpath/reify.git"
  },
  "bugs": {
    "url": "https://github.com/rentpath/reify/issues"
  },
  "homepage": "https://github.com/rentpath/reify#readme",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.0",
    "clear": "0.0.1",
    "clui": "^0.3.6",
    "figlet": "^1.2.0",
    "inquirer": "^4.0.0",
    "preferences": "^1.0.1",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
